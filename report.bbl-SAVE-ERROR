      \strng{namehash}{8be71f42b5c2f081b95f53173c91c492}
      \strng{fullhash}{8be13c29c371b33837fdf502422e1a13}
      \strng{fullhashraw}{8be13c29c371b33837fdf502422e1a13}
      \strng{bibnamehash}{8be71f42b5c2f081b95f53173c91c492}
      \strng{authorbibnamehash}{8be71f42b5c2f081b95f53173c91c492}
      \strng{authornamehash}{8be71f42b5c2f081b95f53173c91c492}
